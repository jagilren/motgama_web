<odoo>
    <record model="report.paperformat" id="formato_tirilla_80">
        <field name="name">formato.tirilla.80</field>
        <field name="default" eval="True"/>
        <field name="format">custom</field>
        <field name="page_width">80</field>
        <field name="page_height">150</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">2</field>
        <field name="margin_right">2</field>
        <field name="margin_bottom">2</field>
        <field name="margin_left">2</field>
        <field name="dpi">90</field>
    </record>

    <record model="report.paperformat" id="formato_tirilla_53">
        <field name="name">formato.tirilla.53</field>
        <field name="default" eval="True"/>
        <field name="format">custom</field>
        <field name="page_width">53</field>
        <field name="page_height">100</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">2</field>
        <field name="margin_right">2</field>
        <field name="margin_bottom">2</field>
        <field name="margin_left">2</field>
        <field name="dpi">90</field>
    </record>
</odoo>