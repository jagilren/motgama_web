<?xml version="1.0" encoding="utf-8"?>
<odoo>
<!--Tree                                                                                                                                    P7.0.4R -->
    <record id="tree_view_habitacion" model="ir.ui.view">
        <field name="name">Movimiento</field>
        <field name="model">motgama.movimiento</field>
        <field name="limit">100</field>
        <field name="arch" type="xml">  
            <tree string="Movimiento" create="false" delete="false" default_order='create_date desc'>
                <field name="habitacion_id"/>
                <field name="tipovehiculo"/>
                <field name="asignafecha"/>
                <field name="asignatipo"/>
            </tree>
        </field>
    </record>  
</odoo>
